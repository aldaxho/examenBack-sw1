{
  "ignore": [
    "temp/*",
    "temp/**/*",
    "node_modules/*",
    "*.zip"
  ],
  "watch": [
    "*.js",
    "routes/**/*.js",
    "controllers/**/*.js",
    "models/**/*.js",
    "middleware/**/*.js",
    "services/**/*.js",
    "utils/**/*.js",
    "config/**/*.js"
  ],
  "ext": "js,json"
}
